{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/vercel/path0",
    "SECURE_SSL_REDIRECT": "true",
    "CSRF_COOKIE_SECURE": "true",
    "SESSION_COOKIE_SECURE": "true"
  }
}